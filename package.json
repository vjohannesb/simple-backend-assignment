{
  "name": "template-backend-backend",
  "version": "1.0.0",
  "description": "",
  "main": "serverless.ts",
  "scripts": {
    "updateD": "node ./update-dependencies.js dependencies",
    "updateDD": "node ./update-dependencies.js devDependencies",
    "test": "jest",
    "lint": "eslint \"**/*.@(js|ts)\"",
    "start": "./node_modules/.bin/serverless offline --noAuth",
