    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.21.0",
    "@types/aws-lambda": "^8.10.108",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "aws-sdk": "^2.1258.0",
    "esbuild": "^0.15.15",
    "eslint": "^8.28.0",
    "jest": "^29.3.1",
    "jest-junit": "^15.0.0",
    "serverless": "^3.24.1",
